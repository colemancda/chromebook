/dts-v1/;

/ {
    description = "Debian kernel image for the Acer Chromebook Tab 10 (scarlet)";

    images {
        kernel@1{
            description = "kernel";
            data = /incbin/("../linux-source-4.19/arch/arm64/boot/Image");
            type = "kernel_noload";
            arch = "arm64";
            os = "linux";
            compression = "none";
            load = <0>;
            entry = <0>;
        };
        fdt@1{
            description = "rk3399-gru-scarlet-inx.dtb";
            data = /incbin/("../linux-source-4.19/arch/arm64/boot/dts/rockchip/rk3399-gru-scarlet-inx.dtb");
            type = "flat_dt";
            arch = "arm64";
            compression = "none";
            hash@1{
                algo = "sha1";
            };
        };
	ramdisk@1{
            description = "initramfs";
            data = /incbin/("/boot/initrd.img-4.19.16");
	    type = "ramdisk";
            arch = "arm";
            os = "linux";
            compression = "none";
            hash@1{
                algo = "sha1";
            };
        };
    };
    configurations {
        default = "conf@1";
        conf@1{
            kernel = "kernel@1";
            fdt = "fdt@1";
	    ramdisk = "ramdisk@1";
        };
    };
};

